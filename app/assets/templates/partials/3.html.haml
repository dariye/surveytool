.page.green
  .info.text-center
    %h1.demo-bold-txt Donor Insights
    %h4(ng-model="questions")
      {{ questions[2] }}
  %p.text-center
    %label(for="no" ng-click="addResponse('demo.four','No')")
      %button.btn.btn-round.btn-yellow
        No
      %input(type="radio" id="no" ng-model="answer" value="No")

    %label(for="yes" ng-click="addResponse('demo.four','Yes')")
      %button.btn.btn-round.btn-light-green
        Yes
      %input(type="radio" id="yes" ng-model="answer" value="Yes")
    %label(for="maybe" ng-click="addResponse('demo.four','Maybe')")
      %button.btn.btn-round.btn-white
        Maybe
      %input(type="radio" id="maybe" ng-model="answer" value="Maybe")