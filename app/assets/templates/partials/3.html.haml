.page.green
  .text-center
    %h1.demo-bold-txt Lorem.Habuit.Suo.
    %h4(ng-model="questions")
      {{ questions[2] }}
    .option-container
      %label.blackshade-text.round-btn.yellow(for="no" ng-click="addResponse('demo.four','No')")
        No
      %input(type="radio" id="no" ng-model="answer" value="No")

      %label.white-text.round-btn.light-green(for="yes" ng-click="addResponse('demo.four','Yes')")
        Yes
      %input(type="radio" id="yes" ng-model="answer" value="Yes")




